{
  "name": "Portfolio",
  "version": "0.0.1",
  "description": "Portfolio for Yifei Gao",
  "keywords": "Portfolio",
  "license": "",
  "author": "Yifei Gao",
  "scripts": {
    "build": "rm -rf dist/ && parcel build index.html --public-url /",
    "dev": "parcel index.html --open",
    "start": "npm run build && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.18"
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      ".css": "css",
      "images": [
        ".jpg",
        ".png"
      ],
      ".fileExtension": "folder/in/dist",
      "folder/in/dist": [
        ".file",
        ".extensions"
      ]
    },
    "options": {
      "development": true
    }
  }
}
